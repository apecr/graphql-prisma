{
  "name": "graphql-prisma",
  "version": "1.1.0-alpha.0",
  "description": "graphql-prisma from https://udemy.com/graphql-bootcamp/",
  "main": "index.js",
  "scripts": {
    "test": "jest --watchAll",
    "start": "node dist/index.js",
    "heroku-postbuild": "babel src --out-dir dist --copy-files",
    "dev": "env-cmd -f ./config/dev.env nodemon src/index.js --ext js,graphql --exec babel-node",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "get-schema": "graphql get-schema -p prisma --dotenv config/dev.env",
    "get-schema-reviews": "graphql get-schema -p prisma-review-website",
    "functional-test": "babel-node test/review-website.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apecr/graphql-prisma.git"
  },
  "keywords": [
    "npm-library"
  ],
  "author": "Alberto Eyo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/apecr/graphql-prisma/issues"
  },
  "homepage": "https://github.com/apecr/graphql-prisma#readme",
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "apollo-server": "^2.1.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "env-cmd": "^10.1.0",
    "graphql": "^14.0.2",
    "graphql-cli": "^3.0.14",
    "graphql-yoga": "^1.16.7",
    "jsonwebtoken": "^8.5.1",
    "prisma-binding": "^2.3.16"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "jest": "^25.1.0",
    "mocha": "^5.2.0",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0"
  }
}
